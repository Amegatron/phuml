<TR>
    <TD BORDER="{{ style.tableBorder }}" ALIGN="LEFT" BGCOLOR="{{ style.methods.background }}">
        {% for method in definition.methods %}
            <FONT COLOR="{{ style.methods.color }}" FACE="{{ style.methods.font }}" POINT-SIZE="{{ style.methods.fontSize }}">
                {{ method }}
            </FONT>
            <BR ALIGN="LEFT"/>
        {% else %}
            &nbsp;
        {% endfor %}
    </TD>
</TR>
