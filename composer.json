{
    "name": "phuml/phuml",
    "type": "library",
    "description": "phUML is a fully automatic UML class diagram generator for PHP code",
    "keywords": [
        "uml"
    ],
    "homepage": "https://github.com/MontealegreLuis/phuml",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-tokenizer": "*",
        "nikic/php-parser": "^4.10.5",
        "padraic/phar-updater": "^1.0.6",
        "symfony/console": "^5.3.0",
        "symfony/filesystem": "^5.3.0",
        "symfony/finder": "^5.3.0",
        "symfony/process": "^5.3.0",
        "twig/twig": "^3.3.2",
        "webmozart/assert": "^1.10.0"
    },
    "require-dev": {
        "ext-imagick": "*",
        "ergebnis/composer-normalize": "^2.14.0",
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "lupka/phpunit-compare-images": "^1.0.0",
        "maglnet/composer-require-checker": "^2.1.0",
        "phpro/grumphp": "^0.19.1",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.88",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "phpunit/phpunit": "^8.5.15",
        "pyrech/composer-changelogs": "^1.7.1"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.34"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "PhUml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhUml\\": [
                "tests/src",
                "tests/integration",
                "tests/unit"
            ]
        }
    },
    "prefer-stable": true,
    "bin": [
        "bin/phuml"
    ],
    "support": {
        "issues": "https://github.com/MontealegreLuis/phuml/issues"
    }
}
