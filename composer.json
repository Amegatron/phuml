{
    "name": "phuml/phuml",
    "type": "library",
    "description": "phUML is a fully automatic UML class diagram generator for PHP code",
    "keywords": [
        "uml"
    ],
    "homepage": "https://github.com/MontealegreLuis/phuml",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-tokenizer": "*",
        "nikic/php-parser": "^4.2.2",
        "padraic/phar-updater": "^1.0.6",
        "symfony/console": "^4.0.9",
        "symfony/filesystem": "^4.0.9",
        "symfony/finder": "^4.0.9",
        "symfony/process": "^4.0.9",
        "twig/twig": "^2.4.8"
    },
    "require-dev": {
        "ext-imagick": "*",
        "friendsofphp/php-cs-fixer": "^2.15.1",
        "lupka/phpunit-compare-images": "^1.0.0",
        "phpstan/phpstan": "^0.11.12",
        "phpunit/phpunit": "^7.5.14"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "PhUml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhUml\\": "tests"
        }
    },
    "prefer-stable": true,
    "bin": [
        "bin/phuml"
    ],
    "support": {
        "issues": "https://github.com/MontealegreLuis/phuml/issues"
    }
}
